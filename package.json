{
  "name": "webui",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.ts",
  "scripts": {
    "dev:client": "vite",
    "dev:electron": "",
    "build:client": "tsc --project tsconfig.json && vite build",
    "build:electron": "tsc --project tsconfig.json && vite build --base=./ && rm -rf dist/electron && cp -r electron/. dist/electron",
    "lint": "eslint .",
    "pack:linux": "npm run build:electron && electron-builder -c.extraMetadata.main=dist/electron/main.ts --publish never",
    "pack:win": "npm run build:electron && electron-builder -c.extraMetadata.main=dist/electron/main.ts --publish never --win",
    "preview:client": "vite preview",
    "preview:electron": ""
  },
  "dependencies": {
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-java": "^6.0.1",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-python": "^6.1.6",
    "@codemirror/lang-xml": "^6.1.0",
    "@mdxeditor/editor": "^3.14.0",
    "bootstrap-icons": "^1.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-force-graph": "^1.44.5",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "is-electron": "^2.2.2",
    "vite": "^5.4.9",
    "vite-plugin-node-polyfills": "^0.22.0"
  },
  "build": {
    "productName": "Oolong",
    "appId": "sh.oolong.app",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "build",
      "output": "release"
    }
  }
}
